 <div class="row">
      <div class="col s12">
            <h2>Orden de ingreso</h2>
      </div>
 </div>
 <div class="hidden" hidden="hidden">
       <input type="text" id="token" value="<%= @c_user.token.token %>">
       <input type="text" id="usuario" value="<%= @c_user.id %>">
       <input type="text" id="siniestro" value="<%= @siniestro.id %>">
       <input type="text" id="urlFotos" value="<%= api_v1_fotos_autos_path %>">
 </div>
 <div class="row">
      <div class="col s6">
            <div class="field">
                  Número Orden:
                  <div class="input-field inline">
                        <input disabled value="<%= @siniestro.noOrden + '/' + @siniestro.noSiniestro %>" id="orden" type="text" class="validate">
                  </div>
            </div>
            <div class="field">
                  Asesor:
                  <div class="input-field inline">
                        <input disabled value="<%= @siniestro.usuario.nombre %>" id="orden" type="text" class="validate">
                  </div>
            </div>
            <div class="field">
                  Cliente:
                  <div class="input-field inline">
                        <input disabled value="<%= @siniestro.cliente.Nombre %>" id="orden" type="text" class="validate">
                  </div>
            </div>
            <div class="field">
                  Dirección:
                  <div class="input-field inline">
                        <input disabled value="<%= @siniestro.cliente.Direccion %>" id="orden" type="text" class="validate">
                  </div>
            </div>
            <div class="field">
                  E-mail:
                  <div class="input-field inline">
                        <input disabled value="<%= @siniestro.cliente.Correo %>" id="orden" type="text" class="validate">
                  </div>
            </div>
            <div class="field">
                  Telefono:
                  <div class="input-field inline">
                        <input disabled value="<%= @siniestro.cliente.Telefono %>" id="orden" type="text" class="validate">
                  </div>
            </div>
      </div>
      <div class="col s6">
            <div class="field">
                  Placas:
                  <div class="input-field inline">
                        <input disabled value="<%= @siniestro.auto.Placas %>" id="orden" type="text" class="validate">
                  </div>
            </div>
            <div class="field">
                  VIN:
                  <div class="input-field inline">
                        <input disabled value="<%= @siniestro.auto.Vin %>" id="orden" type="text" class="validate">
                  </div>
            </div>
            <div class="field">
                  Aseguradora:
                  <div class="input-field inline">
                        <input disabled value="<%= @siniestro.aseguradora.nombre %>" id="orden" type="text" class="validate">
                  </div>
            </div>
            <div class="field">
                  Entrada:
                  <div class="input-field inline">
                        <input disabled value="<%= @siniestro.fechaEntrada %>" id="orden" type="text" class="validate">
                  </div>
            </div>
             <div class="field">
                  Salida:
                  <div class="input-field inline">
                        <input disabled value="<%= @siniestro.fechaSalida %>" id="orden" type="text" class="validate">
                  </div>
            </div>
      </div>
</div>

